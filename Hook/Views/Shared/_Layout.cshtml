</head>
    <body>
        <!-- Header -->
        <header id="header">
            <h1 id="logo"><a href="~/">Hook <span></span></a></h1>
            <nav id="nav">
                <ul>
                    <li class="current"><a href="@Url.RouteUrl("detail", new { ViewBag.Id})">@ViewBag.Id</a></li>
                </ul>
            </nav>
        </header>

        <!-- Main -->
        <article id="main">
            @RenderBody()
        </article>
        
        <!-- Footer -->
        <footer id="footer">


            <ul class="copyright">
                <li>App: <text>@@CurtisMitchell</text></li>
                <li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
            </ul>

        </footer>

        @Scripts.Render("~/bundles/jquery")
            @Scripts.Render("~/bundles/bootstrap")
            @RenderSection("scripts", required: false)
</body>
</html>
